          {
            "file_path": "/home/ec2-user/logs/headless-wallet-install.log",
            "log_group_name": "hc-acc-headless-wallet",
            "log_stream_name": "install_{instance_id}"
          },
          {
            "file_path": "/home/ec2-user/logs/headless-wallet.log",
            "log_group_name": "hc-acc-headless-wallet",
            "log_stream_name": "log_{instance_id}"
          }
        ]
      }
    }
  }
}